{
   "applications": {
      "gecko": {
         "id": "@clipboard-inserter"
      }
   },
   "background": {
      "page": "bg/index.html"
   },
   "browser_action": {
      "browser_style": true,
      "default_icon": {
         "16": "icon/icon16.png",
         "32": "icon/icon32.png",
         "64": "icon/icon64.png"
      },
      "default_title": "Toggle clipboard inserter"
   },
   "description": "A simple addon that inserts clipboard contents into the page. Uses icon made by Google from www.flaticon.com licensed by CC 3.0 BY",
   "icons": {
   },
   "key": "",
   "manifest_version": 2,
   "name": "Clipboard Inserter",
   "options_ui": {
      "page": "options.html"
   },
   "permissions": [ "activeTab", "clipboardRead", "storage", "file://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.3.2"
}
